{
  "format": "page",
  "id": "custom_page",
  "label": "Custom Page",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "cta_section": {
              "type": "SharedSlice"
            },
            "media_section": {
              "type": "SharedSlice"
            },
            "product_categories_section": {
              "type": "SharedSlice"
            },
            "nav_links": {
              "type": "SharedSlice"
            },
            "products_section": {
              "type": "SharedSlice"
            },
            "team_section": {
              "type": "SharedSlice"
            },
            "why_us_section": {
              "type": "SharedSlice"
            },
            "testimonials_section": {
              "type": "SharedSlice"
            },
            "counters_section": {
              "type": "SharedSlice"
            },
            "features_section": {
              "type": "SharedSlice"
            },
            "clients_section": {
              "type": "SharedSlice"
            },
            "gallery_section": {
              "type": "SharedSlice"
            },
            "about_section": {
              "type": "SharedSlice"
            },
            "hero_section": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "Meta Title [40-65 chars]"
        },
        "type": "Text"
      },
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "Page Description [70-155 chars]"
        },
        "type": "Text"
      },
      "featured_image": {
        "type": "Image",
        "config": {
          "label": "Featured Image",
          "constraint": {
            "width": 1200,
            "height": 600
          },
          "thumbnails": [
            {
              "name": "large",
              "width": 1024,
              "height": 524
            },
            {
              "name": "medium",
              "width": 300,
              "height": 150
            },
            {
              "name": "thumbnail",
              "width": 150,
              "height": 150
            }
          ]
        }
      }
    }
  }
}
